
# Program to check if a number is prime using a function

def is_prime(n):
    if n <= 1:
        return False
    for i in range(2, int(n ** 0.5) + 1):
        if n % i == 0:
            return False
    return True

num = int(input("Enter a number: "))
print("Prime Number" if is_prime(num) else "Not a Prime Number")
